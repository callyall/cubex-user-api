{
  "name": "cubex/dist",
  "license": "MIT",
  "type": "project",
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "prefer-stable": true,
  "require": {
    "ext-json": "*",
    "cubex/framework": "^4.0",
    "filp/whoops": "^2.3",
    "firebase/php-jwt": "^5.2",
    "packaged/dal": "^1.8",
    "packaged/dispatch": "~2.6",
    "packaged/form": "~2.13.0"
  },
  "require-dev": {
    "phpunit/phpunit": "7.5.*"
  },
  "autoload": {
    "psr-4": {
      "UserApi\\": "src/"
    }
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "git@github.com:cubex/framework.git"
    }
  ]
}
