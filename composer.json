{
  "name": "cubex/dist",
  "license": "MIT",
  "type": "project",
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "prefer-stable": true,
  "require": {
    "php": ">=8.2",
    "ext-json": "*",
    "cubex/framework": "^4.0",
    "nesbot/carbon": "^2.71",
    "protectednet/dependency-resolver": "^1.1.0"
  },
  "require-dev": {
    "filp/whoops": "^2.15.2",
    "phpunit/phpunit": "^10.1.3"
  },
  "autoload": {
    "psr-4": {
      "UserApi\\": "src/",
      "UserApi\\Tests\\": "tests/"
    }
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "git@github.com:cubex/framework.git"
    },
    {
      "type": "git",
      "url": "git@github.com:protectednet/dependency-resolver.git"
    }
  ]
}
